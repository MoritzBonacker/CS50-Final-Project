pod 'Firebase/Auth'

  target 'CS50 Final ProjectTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CS50 Final ProjectUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
